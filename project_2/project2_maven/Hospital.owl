<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:ds="https://data.medicare.gov/d/nrth-mfg3#"
    xmlns:du="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Class rdf:about="https://data.medicare.gov/d/nrth-mfg3#Hospital"/>
  <owl:Class rdf:about="https://data.medicare.gov/d/nrth-mfg3#State">
    <rdfs:comment xml:lang="EN">One of 50 states in US</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://data.medicare.gov/d/nrth-mfg3#Statistics2018"/>
  <owl:Class rdf:about="https://data.medicare.gov/d/nrth-mfg3#Nation">
    <rdfs:comment xml:lang="EN">A large body of people united by common descent, history, culture, or language, inhabiting a particular country or territory.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#isScoreOf">
    <rdfs:comment xml:lang="EN">is a hospital's score</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#isHospitalAverageSpendingOf">
    <rdfs:comment xml:lang="EN">is a hospital's average spending per Beneficiary (MSPB) episodes in USD of</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#hasScore">
    <rdfs:domain rdf:resource="https://data.medicare.gov/d/nrth-mfg3#Statistics2018"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:comment xml:lang="EN">has a hospital's score</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#isRatingOf">
    <rdfs:comment xml:lang="EN">is a hospital's rating from 1-5</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#isEmergencyServiceOf">
    <rdfs:comment xml:lang="EN">is a boolean whether a facility has emergency service</rdfs:comment>
    <owl:inverseOf>
      <owl:FunctionalProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#hasFacilityName"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#hasRating">
    <rdfs:domain rdf:resource="https://data.medicare.gov/d/nrth-mfg3#Statistics2018"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >5</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
          >1</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:comment xml:lang="EN">has a hospital's rating from 1-5</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#hasHospitalAverageSpending">
    <rdfs:domain rdf:resource="https://data.medicare.gov/d/nrth-mfg3#Statistics2018"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:comment xml:lang="EN">has a hospital's average spending per Beneficiary (MSPB) episodes in USD</rdfs:comment>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#hasID">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    <rdfs:comment xml:lang="EN">has a particular facility ID</rdfs:comment>
    <rdfs:domain rdf:resource="https://data.medicare.gov/d/nrth-mfg3#Hospital"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#hasPhoneNumber">
    <rdfs:domain rdf:resource="https://data.medicare.gov/d/nrth-mfg3#Hospital"/>
    <rdfs:comment xml:lang="EN">A phone number of a facility</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#hasEmergencyService">
    <rdfs:domain rdf:resource="https://data.medicare.gov/d/nrth-mfg3#Hospital"/>
    <rdfs:comment xml:lang="EN">A boolean whether a facility has emergency service</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#hasFacilityName">
    <rdfs:domain rdf:resource="https://data.medicare.gov/d/nrth-mfg3#Hospital"/>
    <rdfs:comment xml:lang="EN">has the facility's name</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:FunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#isIDOf">
    <owl:inverseOf rdf:resource="https://data.medicare.gov/d/nrth-mfg3#hasID"/>
    <rdfs:comment xml:lang="EN">is facility ID of</rdfs:comment>
  </owl:InverseFunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="https://data.medicare.gov/d/nrth-mfg3#isFacilityNameOf">
    <rdfs:comment xml:lang="EN">is a boolean whether a facility has emergency service</rdfs:comment>
    <owl:inverseOf rdf:resource="https://data.medicare.gov/d/nrth-mfg3#hasFacilityName"/>
    <rdfs:comment xml:lang="EN">is facility's name of</rdfs:comment>
  </owl:InverseFunctionalProperty>
  <ds:Hospital rdf:about="https://data.medicare.gov/d/nrth-mfg3#Liam">
    <ds:hasID>1234</ds:hasID>
    <ds:hasFacilityName>Liam</ds:hasFacilityName>
  </ds:Hospital>
</rdf:RDF>
